{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "cli": {
    "analytics": false
  },
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "ngweb": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        },
        "@schematics/angular:application": {
          "strict": true
        }
      },
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "docs",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.app.json",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "./src/assets/styles.scss",
              "./src/assets/vendor/fontawesome-free/css/all.min.css",
              "./src/assets/css/sb-admin-2.css",
              "./src/assets/css/datatable.css",
              "./src/assets/vendor/datatables/dataTables.bootstrap4.min.css",
              "./src/assets/vendor/datatables/buttons.bootstrap4.min.css",
              "./node_modules/ngx-toastr/toastr.css"
            ],
            "scripts": [
              "./src/assets/vendor/jquery/jquery.min.js",
              "./src/assets/vendor/bootstrap/js/bootstrap.bundle.min.js",
              "./src/assets/vendor/jquery-easing/jquery.easing.min.js",
              "./src/assets/js/sb-admin-2.min.js",
              "./src/assets/vendor/datatables/jquery.dataTables.min.js",
              "./src/assets/vendor/datatables/dataTables.bootstrap4.min.js",
              "./src/assets/vendor/datatables/dataTables.buttons.min.js",
              "./src/assets/vendor/datatables/buttons.html5.min.js",
              "./src/assets/vendor/datatables/buttons.bootstrap4.min.js",
              "./src/assets/vendor/datatables/jszip.min.js",
              "./src/assets/vendor/datatables/pdfmake.min.js",
              "./src/assets/vendor/datatables/vfs_fonts.js",
              "./src/assets/vendor/datatables/buttons.print.min.js",
              "./src/assets/vendor/datatables/buttons.colVis.min.js",
              "./src/assets/vendor/datatables/dataTables.bootstrap4.min.js",
              "./src/assets/js/demo/datatables-demo.js"
            ]
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2.5mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2.5mb",
                  "maximumError": "5mb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "ngweb:build:production"
            },
            "development": {
              "browserTarget": "ngweb:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "ngweb:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.spec.json",
            "karmaConfig": "karma.conf.js",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "./node_modules/@angular/material/prebuilt-themes/pink-bluegrey.css",
              "src/styles.css"
            ],
            "scripts": []
          }
        }
      }
    }
  }
}
